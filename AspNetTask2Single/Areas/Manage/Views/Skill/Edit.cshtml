@model Skills
@{
    ViewData["Title"] = "Edit";
}

<div class="container-fluid">
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            
            <div class="form-group col-md-6">
                <label asp-for="WorkFlow">WorkFlow</label>
                <input asp-for="WorkFlow" class="form-control" placeholder="WorkFlow">
            </div>
            <div>
                <label for="formFileLg" class="form-label">Upload File</label>
                <input asp-for="Photo" class="form-control" type="file" />
                <span class="text-danger" asp-validation-for="Photo"></span>
            </div>
        </div>
        
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Edit</button>
        </div>
    </form>
</div>

